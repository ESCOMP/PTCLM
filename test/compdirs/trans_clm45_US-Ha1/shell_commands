# ../PTCLMmkdata --mksurfdata_opts=-no_crop --verbose --pftgrid --clmnmlusecase=20thC_transient --cesm_root ../../../../.. -s US-Ha1 -d /glade/p/cesmdata/cseg/inputdata --debug --sdate 140204 --map_gdate 140204 --mydatadir testing_dir/clm45grd_trans_pft 
